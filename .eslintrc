{
  "root": true,
  "parser": "babel-eslint",
  "plugins": [
    "import",
    "jsx-a11y"
  ],
  "extends": [
    "eslint:recommended"
  ],
  "env": {
    "node": true,
    "browser": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "modules": true
    }
  },
  "rules": {
    "no-set-state": "off",
    "no-console": "off"
  }
}
